.UCenter-bg {
  background-size: cover;
  height: 450rpx;
  display: flex;
  justify-content: center;
  padding-top: 150rpx;
  overflow: hidden;
  position: relative;
  flex-direction: column;
  align-items: center;
  color: #fff;
  font-weight: 300;
  text-shadow: 0 0 3px rgba(0, 0, 0, 0.3);
}
.UCenter-bg text {
  opacity: 0.8;
}

.UCenter-bg .avatar {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}

.UCenter-bg .gif-wave {
  position: absolute;
  width: 100%;
  bottom: 0;
  left: 0;
  z-index: 99;
  mix-blend-mode: screen;
  height: 100rpx;
}
/* 图标 */
.cu-list.menu > .cu-item .content > image {
  display: inline-block;
  margin-left: 8rpx;
  margin-right: 12rpx;
  width: 1em;
  height: 1em;
  vertical-align: middle;
}

.dialog-list {
  margin-bottom: 130rpx;
}